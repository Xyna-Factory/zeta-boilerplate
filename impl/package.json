{
    "name": "xyna",
    "title": "My Xyna Project",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "ng": "ng",
        "start": "ng serve --configuration=development --ssl=true --ssl-key=./ssl/server-public.pem",
        "build": "ng build --configuration=development",
        "release": "ng build xyna --configuration=production --extract-licenses",
        "lint": "eslint --color -c .eslintrc.json --ext .ts ./projects/xyna/src/app",
        "lint:fix": "npm run lint -- --fix",
        "tsc": "tsc",
        "extract-i18n": "ng xi18n xyna --i18n-format xlf --output-path ./locale --i18n-locale en-US && ng run xyna:xliffmerge"
    },
    "dependencies": {
        "@angular/animations": "16.2.10",
        "@angular/cdk": "16.2.10",
        "@angular/common": "16.2.10",
        "@angular/compiler": "16.2.10",
        "@angular/core": "16.2.10",
        "@angular/forms": "16.2.10",
        "@angular/localize": "16.2.10",
        "@angular/material": "16.2.10",
        "@angular/platform-browser": "16.2.10",
        "@angular/platform-browser-dynamic": "16.2.10",
        "@angular/router": "16.2.10",
        "@angular/service-worker": "16.2.10",
        "escape-string-regexp": "5.0.0",
        "hammerjs": "2.0.8",
        "rxjs": "7.5.6",
        "three": "0.129.0",
        "tslib": "2.4.0",
        "zone.js": "0.13.3"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "16.2.7",
        "@angular-eslint/eslint-plugin": "15.2.1",
        "@angular/cli": "16.2.7",
        "@angular/compiler-cli": "16.2.10",
        "@angular/language-service": "16.2.10",
        "@types/node": "18.0.6",
        "@types/offscreencanvas": "2019.7.0",
        "@types/resize-observer-browser": "0.1.7",
        "@types/three": "0.129.1",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "eslint": "8.51.0",
        "eslint-plugin-import": "2.28.1",
        "eslint-plugin-jsdoc": "46.8.2",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "eslint-plugin-zeta": "file:projects/xyna/src/app/zeta/lint/plugins/eslint-plugin-zeta",
        "ts-node": "10.9.1",
        "typescript": "4.9.5"
    }
}